root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.cdc]
indent_style = space
indent_size = 4

[*.key]
insert_final_newline = false

[Makefile]
indent_style = tab
